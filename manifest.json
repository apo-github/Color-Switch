{
    "manifest_version": 3, 
    "version": "1.0", 
    "name": "change the specific page color", 
    "description": "change the specific page color",
    "permissions":["scripting", "activeTab", "storage", "tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "./scripts/content-script.js"
            ],
            "type": "module"
        }
    ],
    "action": {
        "default_popup": "./view/popup.html"
    }
}